type Player {
    id: Int
    name: String
}

type Game {
    id: Int
    player_id: Int
    rounds: [Round]
}

type Round {
    id: Int
    human_choice: String
    computer_choice: String
    winner: String
}

type Query {
    findAllPlayers: [Player]
}

type Mutation {
    createPlayer(name: String!): Player
    createGame(game: Game!): Game
}